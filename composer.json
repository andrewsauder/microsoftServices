{
  "name": "andrewsauder/microsoft-services",
  "description": "PHP wrapper for easy implementation of Microsoft Graph services",
  "license": "MIT",
  "autoload": {
	"psr-4": {
	  "andrewsauder\\microsoftServices\\": "src/"
	}
  },
  "require": {
	"php": ">=8.2",
	"microsoft/microsoft-graph": "^1.110",
	"guzzlehttp/guzzle": "^7.9",
	"thenetworg/oauth2-azure": "^2.0",
	"ext-fileinfo": "*",
	"ext-openssl": "*"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "tbachert/spi": true
    }
  }
}
